  "devDependencies": {
    "@swc-node/register": "1.10.9",
    "@types/express": "4.17.21",
    "@types/node": "22.10.0",
    "@types/superagent": "8.1.9",
    "@types/supertest": "6.0.2",
    "@types/whatwg-url": "13.0.0",
    "c8": "10.1.2",
    "express": "4.21.1",
    "io-ts-types": "0.5.19",
    "superagent": "10.1.1",
    "supertest": "7.0.0",
    "typescript": "4.7.4"
