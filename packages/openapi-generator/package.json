{
  "name": "@api-ts/openapi-generator",
  "version": "0.0.0-semantically-released",
  "description": "Generate an OpenAPI specification from an io-ts-http contract",
  "author": "Patrick McLaughlin <patrickmclaughlin@bitgo.com>",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "files": [
    "dist/src/"
  ],
  "bin": "./dist/src/cli.js",
  "scripts": {
    "build": "tsc --build --incremental --verbose .",
    "clean": "rm -rf -- dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "c8 --all --src src node --loader tsx --test test/test-entrypoint.ts"
  },
  "dependencies": {
    "@api-ts/io-ts-http": "0.0.0-semantically-released",
    "cmd-ts": "0.13.0",
    "comment-parser": "1.4.0",
    "fp-ts": "2.16.1",
    "io-ts": "2.1.3",
    "io-ts-types": "0.5.19",
    "openapi-types": "12.1.3",
    "ts-morph": "14.0.0",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "c8": "8.0.1",
    "parser-ts": "0.7.0",
    "tsx": "3.12.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
