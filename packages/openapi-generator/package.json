{
  "name": "@api-ts/openapi-generator",
  "version": "0.0.0-semantically-released",
  "description": "Generate an OpenAPI specification from an io-ts-http contract",
  "author": "Patrick McLaughlin <patrickmclaughlin@bitgo.com>",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "files": [
    "dist/src/"
  ],
  "bin": "./dist/src/cli.js",
  "scripts": {
    "build": "tsc --build --incremental --verbose .",
    "clean": "rm -rf -- dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "c8 --all --src src node --require @swc-node/register --test test/*.test.ts"
  },
  "dependencies": {
    "@swc/core": "1.5.0",
    "cmd-ts": "0.13.0",
    "comment-parser": "1.4.1",
    "fp-ts": "2.16.5",
    "io-ts": "2.1.3",
    "io-ts-types": "0.5.19",
    "openapi-types": "12.1.3",
    "resolve": "1.22.8"
  },
  "devDependencies": {
    "@swc-node/register": "1.9.0",
    "@types/resolve": "1.20.6",
    "c8": "9.1.0",
    "memfs": "4.8.2",
    "typescript": "4.7.4"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "1.5.0",
    "@swc/core-darwin-arm64": "1.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
