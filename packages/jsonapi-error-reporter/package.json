{
  "name": "@api-ts/jsonapi-error-reporter",
  "version": "0.0.0-semantically-released",
  "description": "io-ts path reporter for emitting errors in JSON API error specification format",
  "author": "Eric Crosson <ericcrosson@bitgo.com>",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/src/"
  ],
  "scripts": {
    "build": "tsc --build --incremental --verbose .",
    "clean": "rm -rf -- dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "npm run build && c8 --all node --require @swc-node/register --test test/*.test.ts",
    "example": "node --require @swc-node/register example/api.ts"
  },
  "dependencies": {
    "fp-ts": "^2.0.0",
    "io-ts": "2.1.3"
  },
  "devDependencies": {
    "@api-ts/io-ts-http": "0.0.0-semantically-released",
    "@api-ts/typed-express-router": "0.0.0-semantically-released",
    "@api-ts/express-wrapper": "0.0.0-semantically-released",
    "@api-ts/superagent-wrapper": "0.0.0-semantically-released",
    "@swc-node/register": "1.10.9",
    "@types/express": "4.17.21",
    "c8": "10.1.3",
    "express": "4.21.2",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
